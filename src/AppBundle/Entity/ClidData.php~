<?php

namespace AppBundle\Entity;

/**
 * ClidData
 */
class ClidData
{
    /**
     * @var integer
     */
    private $idClid;

    /**
     * @var string
     */
    private $forma;


    /**
     * Get idClid
     *
     * @return integer
     */
    public function getIdClid()
    {
        return $this->idClid;
    }

    /**
     * Set forma
     *
     * @param string $forma
     *
     * @return ClidData
     */
    public function setForma($forma)
    {
        $this->forma = $forma;

        return $this;
    }

    /**
     * Get forma
     *
     * @return string
     */
    public function getForma()
    {
        return $this->forma;
    }

    /**
     * Set idClid
     *
     * @param integer $idClid
     *
     * @return ClidData
     */
    public function setIdClid($idClid)
    {
        $this->idClid = $idClid;

        return $this;
    }
}
